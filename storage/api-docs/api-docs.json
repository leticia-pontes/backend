                ],
                "summary": "Cria uma nova empresa",
                "operationId": "bea7e1cd70745d9a85563a4cdba64902",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nome",
                                    "cnpj",
                                    "email",
                                    "senha"
                                ],
                                "properties": {
                                    "nome": {
                                        "type": "string",
                                        "example": "Empresa Exemplo"
                                    },
                                    "cnpj": {
                                        "type": "string",
                                        "example": "12.345.678/0001-99"
                                    },
                                    "perfil": {
                                        "type": "string",
                                        "example": "perfil_exemplo"
                                    },
                                    "seguidores": {
                                        "type": "integer",
                                        "example": 100
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "email@exemplo.com"
                                    },
                                    "senha": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "123456"
                                    },
                                    "telefone": {
                                        "type": "string",
                                        "example": "(11) 99999-9999"
                                    },
                                    "endereco": {
                                        "type": "string",
                                        "example": "Rua Exemplo, 123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Empresa criada com sucesso"
                    }
                }
            }
        },
