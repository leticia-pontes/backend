{
    "openapi": "3.0.0",
    "info": {
        "title": "API backend",
        "description": "Documentação da API",
        "version": "1.0.0"
    },
    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Autenticação"
                ],
                "summary": "Login de empresa",
                "operationId": "dd51a484b7fa0f97c7680fdbde14cf5f",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "senha"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email"
                                    },
                                    "senha": {
                                        "type": "string",
                                        "format": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login efetuado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "empresa": {
                                            "type": "object"
                                        },
                                        "token": {
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Credenciais inválidas"
                    }
                }
            }
        },
        "/api/auth/register": {
            "post": {
                "tags": [
                    "Autenticação"
                ],
                "summary": "Cadastro de empresa",
                "operationId": "c7bb21a0d87049e161b8e4c9b0d909f2",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nome",
                                    "email",
                                    "senha",
                                    "senha_confirmation",
                                    "perfil"
                                ],
                                "properties": {
                                    "nome": {
                                        "type": "string",
                                        "example": "Empresa Exemplo"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "empresa@exemplo.com"
                                    },
                                    "senha": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "senha123"
                                    },
                                    "senha_confirmation": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "senha123"
                                    },
                                    "perfil": {
                                        "type": "string",
                                        "enum": [
                                            "empresa_contratante",
                                            "desenvolvedor"
                                        ],
                                        "example": "empresa_contratante"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Cadastro realizado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Cadastro realizado com sucesso"
                                        },
                                        "empresa": {
                                            "type": "object"
                                        },
                                        "access_token": {
                                            "type": "string",
                                            "example": "1|xpto123..."
                                        },
                                        "token_type": {
                                            "type": "string",
                                            "example": "Bearer"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Erro de validação",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "errors": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/empresas": {
            "get": {
                "tags": [
                    "Empresa"
                ],
                "summary": "Lista todas as empresas",
                "operationId": "9239adbd1cfe29bdf0372db2124d0b1f",
                "responses": {
                    "200": {
                        "description": "Lista de Empresas",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Empresa"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Empresa"
                ],
                "summary": "Cria uma nova empresa",
                "operationId": "bea7e1cd70745d9a85563a4cdba64902",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nome",
                                    "cnpj",
                                    "email",
                                    "senha"
                                ],
                                "properties": {
                                    "nome": {
                                        "type": "string",
                                        "example": "Empresa Exemplo"
                                    },
                                    "cnpj": {
                                        "type": "string",
                                        "example": "12.345.678/0001-99"
                                    },
                                    "perfil": {
                                        "type": "string",
                                        "example": "perfil_exemplo"
                                    },
                                    "seguidores": {
                                        "type": "integer",
                                        "example": 100
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "email@exemplo.com"
                                    },
                                    "senha": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "123456"
                                    },
                                    "telefone": {
                                        "type": "string",
                                        "example": "(11) 99999-9999"
                                    },
                                    "endereco": {
                                        "type": "string",
                                        "example": "Rua Exemplo, 123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Empresa criada com sucesso"
                    }
                }
            }
        },
        "/api/empresas/{id}": {
            "get": {
                "tags": [
                    "Empresa"
                ],
                "summary": "Mostra uma empresa pelo id",
                "operationId": "7e0d938df2a2305cd42451cc84ec72ec",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID da empresa",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Detalhes da empresa",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Empresa"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Empresa não encontrada"
                    }
                }
            },
            "put": {
                "tags": [
                    "Empresa"
                ],
                "summary": "Atualiza uma empresa",
                "operationId": "61d6235f805bd62afcf89b24a6bae3aa",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID da empresa",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/EmpresaUpdateRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Empresa atualizada",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Empresa"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Empresa não encontrada"
                    },
                    "422": {
                        "description": "Erro de validação"
                    }
                }
            },
            "delete": {
                "tags": [
                    "Empresa"
                ],
                "summary": "Deleta uma empresa",
                "operationId": "ba97e99dc7744b02f562b861b32e29a6",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID da empresa",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Empresa deletada com sucesso"
                    },
                    "404": {
                        "description": "Empresa não encontrada"
                    }
                }
            }
        },
