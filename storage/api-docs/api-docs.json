                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Perfil"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Perfil não encontrado"
                    },
                    "422": {
                        "description": "Erro de validação"
                    }
                }
            },
            "delete": {
                "tags": [
                    "Perfil"
                ],
                "summary": "Remove um perfil",
                "operationId": "ece61c4ab475880a9f277d1439adeb10",
